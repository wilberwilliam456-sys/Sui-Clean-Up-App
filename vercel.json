{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "VITE_API_URL": "https://your-backend-url.vercel.app"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.vercel.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}